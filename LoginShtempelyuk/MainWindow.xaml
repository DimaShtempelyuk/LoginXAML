        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoginShtempelyuk"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="tb" HorizontalAlignment="Left" Height="20" Margin="40,70,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
